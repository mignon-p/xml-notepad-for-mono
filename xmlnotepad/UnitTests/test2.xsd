      <xs:documentation>This is a sample schema that demonstrates the various types of intellisense supported by XML Notepad 2007.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence maxOccurs="2">
        <xs:element name="color" type="color" />
        <xs:element name="date" type="xs:date" />
        <xs:element name="dateTime" type="xs:dateTime" />
        <xs:element name="photo" type="xs:anyURI"/>
        <xs:choice maxOccurs="5">
          <xs:element name="fruit" type="fruits" />
          <xs:element name="vegetable" type="veges" />
          <xs:element name="berry" type="berries" />
          <xs:element name="font" type="font" />
        </xs:choice>
        <xs:element name="weight" type="xs:decimal" />
      </xs:sequence>
      <xs:attribute name="language" type="xs:language" />
      <xs:attribute name="sold" type="xs:boolean" />
      <xs:attribute name="time" type="xs:time" />
    </xs:complexType>
  </xs:element>
